<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onnoa.shop.demo.video.mapper.TransactionRollbackMapper">


    <insert id="insertEntity">
        insert into transaction_rollback (`name`, `desc`, `phone`) values (#{entity.name}, #{entity.desc},#{entity.phone} )
    </insert>

    <select  id="queryTest"   parameterType="Map" resultType="Map">
        select * from transaction_rollback WHERE name =#{name}
    </select>
</mapper>
